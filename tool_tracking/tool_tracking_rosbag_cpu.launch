      	<node name="vesselness_view_left" pkg="vesselness_image_filter_cpu" type="vesselness_image_filter_cpu_viewer_node">
         <remap from="image_thin" to="/davinci/left/image_vesselness"/>
        </node>

        <node name="vesselness_view_right" pkg="vesselness_image_filter_cpu" type="vesselness_image_filter_cpu_viewer_node">
         <remap from="image_thin" to="/davinci/right/image_vesselness"/>
        </node>

